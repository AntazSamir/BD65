# Supabase Configuration
# Get these values from your Supabase project dashboard

# Supabase URL (e.g., https://your-project.supabase.co)
SUPABASE_URL=your_supabase_url_here

# Supabase Anonymous Key (public key for client-side usage)
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Database Connection String for Drizzle
# Format: postgresql://postgres:[password]@[host]:5432/postgres
# Example: postgresql://postgres:your_password@db.your-project.supabase.co:5432/postgres
DATABASE_URL=your_supabase_database_url_here

# Optional: Supabase database password (used for fallback connection)
SUPABASE_DB_PASSWORD=your_database_password_here

# Node Environment
NODE_ENV=development

# Server Port (optional, defaults to 5000)
PORT=5000

# Session Configuration (for express-session)
SESSION_SECRET=your_session_secret_here

# Optional: Additional Supabase Configuration
# SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here  # For admin operations
